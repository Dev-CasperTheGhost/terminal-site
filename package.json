{
  "name": "terminal-site",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .tsx,.ts,.jsx,.js",
    "lint:fix": "eslint ./src --fix --ext .tsx,.ts,.jsx,.js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-bootstrap-icons": "^1.8.2",
    "react-dom": "^18.1.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^4.4.0",
    "@casper124578/eslint-config-react": "^4.4.0",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  }
}
